{
  "compilerOptions": {
    "module": "CommonJS",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "declaration": false,
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "target": "es2018",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "typeRoots": ["node_modules/@types", "src/types"],
    "baseUrl": "./",
    "paths": {
      "@NodeService/*": ["src/*"]
    }
  },
  "compileOnSave": true,
}
